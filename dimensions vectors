values <- 1:24
dimensions <- c(3, 4, 2)
dim_names <- list(
  Dimension1 = c("Row1", "Row2", "Row3"),
  Dimension2 = c("Col1", "Col2", "Col3", "Col4"),
  Dimension3 = c("Layer1", "Layer2")
)
array_example <- array(values, dim = dimensions, dimnames = dim_names)
print("Array with Dimension Names:")
print(array_example)
