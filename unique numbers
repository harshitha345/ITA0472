get_unique_numbers <- function(input_vector) {
  unique_numbers <- unique(input_vector)
  return(unique_numbers)
}
input_vector <- c(1, 2, 3, 1, 3, 4, 5, 2)
unique_numbers <- get_unique_numbers(input_vector)
print(paste("Unique numbers of vector", paste(input_vector, collapse = ", "), "are:", paste(unique_numbers, collapse = ", ")))
